{% macro print(config) -%}
{%   for line in config.split('\n') | sort %}
{%     if line != '' %}
{{ line }};
{%     endif %}
{%   endfor %}
{%- endmacro %}

{% macro print_all(pools) -%}
{%   for pool, config in pools.iteritems() | sort %}
pool {
{{     print(config) | indent(4, true) }}
}
{%   endfor %}
{%- endmacro %}